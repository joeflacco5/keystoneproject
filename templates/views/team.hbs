<div class='container'>
<div class="row">
  <div class="col-xs-12">
    {{# each data.teams}}
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="panel-title">{{name}}</div>
      </div>
      <div class="panel-body">
        <p>{{image}}</p>
      </div>
      <div class="panel-footer">
        <a href="{{url}}" target="_blank" class="btn btn-primary">Team Website</a>
      </div>
    </div>
    {{/each}}
  </div>
</div>
</div>
