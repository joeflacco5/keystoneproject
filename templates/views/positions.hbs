<div class='container'>
<div class="row">
  <div class="col-xs-12">
    {{# each data.positions}}
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="panel-title">{{position}}</div>
      </div>
      <div class="panel-body">
        {{# each players}}
        <ul>
        <li>{{name}}</li>
      </ul>
        {{/each}}
      </div>
      <div class="panel-footer">
      </div>
    </div>
    {{/each}}
  </div>
</div>
</div>
